<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>ByRunId</name>
    <description>append rows from etl_source to etl_target and etl_target2 if transfer rowid less than source diTransformRunId</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="vehicle" queryName="etl_source" />
            <destination schemaName="vehicle" queryName="etl_target" />
        </transform>
    </transforms>
    <incrementalFilter className="RunFilterStrategy" runTableSchema="vehicle" runTable="Transfer" pkColumnName="Rowid" fkColumnName= "TransformRun" />
    <schedule>
    </schedule>
</etl>