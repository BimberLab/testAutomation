<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Merge Test</name>
    <description>merge rows from etl_source to etl_target and etl target 2</description>
    <transforms>
        <transform id="merge" type="org.labkey.di.pipeline.TransformTask">
            <description>Merge to target</description>
            <source schemaName="vehicle" queryName="etl_source" />
            <destination schemaName="vehicle" queryName="etl_target" targetOption="merge"/>
        </transform>
        <transform id="merge2" type="org.labkey.di.pipeline.TransformTask">
            <description>Merge to target 2</description>
            <source schemaName="vehicle" queryName="etl_source" />
            <destination schemaName="vehicle" queryName="etl_target2" targetOption="merge"/>
        </transform>
    </transforms>
    <schedule>
    </schedule>
</etl>
