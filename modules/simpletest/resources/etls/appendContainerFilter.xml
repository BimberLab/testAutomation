<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Append Test With Container Filter</name>
<description>append rows from etl_source to etl_target, using a container filter</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="vehicle" queryName="etl_source" containerFilter="CurrentAndSubfolders"/>
        <destination schemaName="vehicle" queryName="etl_target" />
    </transform>
</transforms>
<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="modified"/>
</etl>
