<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>AppendWithRowversion</name>
<description>append rows from etl_source to etl_target, use rowversion column for filter</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="vehicle" queryName="etl_source" />
        <destination schemaName="vehicle" queryName="etl_target" />
    </transform>
</transforms>
<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="rowversion">
    <deletedRowsSource schemaName="vehicle" queryName="etl_delete" deletedSourceKeyColumnName="id" timestampColumnName="created" targetKeyColumnName="id"/>
</incrementalFilter>
    <schedule>
        <poll interval="15s" />
    </schedule>
</etl>
