<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Constants No Matching Source Test</name>
    <description>Use both global constant override with no matching source column name</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="vehicle" queryName="etl_180column_source" />
            <destination schemaName="vehicle" queryName="etl_target">
            </destination>
        </transform>
    </transforms>
    <constants>
        <column name="name" type="VARCHAR" value="addThisName"/>
    </constants>    
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="modified"/>
</etl>
