<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Write to File From Query</name>
<description>Write rows from etl_source to a file</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="nlp" queryName="OBX" />
        <destination type="file" filePath="etlOut" fileBaseName="etl_targetOBX-#" fileExtension="tsv"/>
    </transform>
</transforms>
<incrementalFilter className="SelectAllFilterStrategy" />
    <!--<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="modified" />-->
    <schedule>
        <poll interval="31s" />
    </schedule>
</etl>
