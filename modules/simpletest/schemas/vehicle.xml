<ns:tables xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <ns:table tableName="Colors" tableDbType="TABLE">
    <ns:pkColumnName>Name</ns:pkColumnName>
    <ns:columns>
      <ns:column columnName="Name">
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>30</ns:scale>
      </ns:column>
      <ns:column columnName="Hex">
        <ns:nullable>true</ns:nullable>
        <ns:datatype>varchar</ns:datatype>
      </ns:column>
    </ns:columns>
  </ns:table>

  <ns:table tableName="Manufacturers" tableDbType="TABLE">
    <ns:pkColumnName>RowId</ns:pkColumnName>
    <ns:auditLogging>SUMMARY</ns:auditLogging>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:datatype>serial</ns:datatype>
        <ns:isAutoInc>true</ns:isAutoInc>
        <ns:scale>10</ns:scale>
        <ns:nullable>false</ns:nullable>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isKeyField>true</ns:isKeyField>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Name">
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>255</ns:scale>
      </ns:column>
    </ns:columns>
  </ns:table>

  <ns:table tableName="Models" tableDbType="TABLE">
    <ns:pkColumnName>RowId</ns:pkColumnName>
    <ns:auditLogging>DETAILED</ns:auditLogging>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:datatype>serial</ns:datatype>
        <ns:isAutoInc>true</ns:isAutoInc>
        <ns:scale>10</ns:scale>
        <ns:nullable>false</ns:nullable>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isKeyField>true</ns:isKeyField>
        <ns:isHidden>true</ns:isHidden>
        <ns:url>/simpletest/model.view?rowid=${RowId}</ns:url>
      </ns:column>
      <ns:column columnName="Name">
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>255</ns:scale>
        <ns:url>/simpletest/model.view?rowid=${RowId}</ns:url>
      </ns:column>
      <ns:column columnName="ManufacturerId">
        <ns:columnTitle>Manufacturer</ns:columnTitle>
        <ns:fk>
          <ns:fkColumnName>RowId</ns:fkColumnName>
          <ns:fkTable>Manufacturers</ns:fkTable>
          <ns:fkDbSchema>simple</ns:fkDbSchema>
        </ns:fk>
        <ns:datatype>int</ns:datatype>
      </ns:column>
    </ns:columns>
  </ns:table>

  <ns:table tableName="Vehicles" tableDbType="TABLE">
    <ns:pkColumnName>RowId</ns:pkColumnName>
    <ns:tableUrl>/simpletest/vehicle.view?rowid=${RowId}</ns:tableUrl>
    <ns:importMessage>Hello.  Please read this before you import data</ns:importMessage>
    <ns:importTemplates>
        <ns:template label="Template 1" url="/query/exportExcelTemplate.view?schemaName=vehicle&amp;query.queryName=Vehicles&amp;captionType=Label&amp;filenamePrefix=vehicles" />
        <ns:template label="Template 2" url="/query/exportExcelTemplate.view?schemaName=vehicle&amp;query.queryName=Vehicles&amp;captionType=Label&amp;filenamePrefix=vehicles&amp;query.viewName=AlternateImportTemplate" />
    </ns:importTemplates>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:datatype>serial</ns:datatype>
        <ns:isAutoInc>true</ns:isAutoInc>
        <ns:scale>10</ns:scale>
        <ns:nullable>false</ns:nullable>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isKeyField>true</ns:isKeyField>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Container">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>entityid</ns:datatype>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="CreatedBy">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>userid</ns:datatype>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isHidden>true</ns:isHidden>
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
      </ns:column>
      <ns:column columnName="Created">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>timestamp</ns:datatype>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="ModifiedBy">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>userid</ns:datatype>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isHidden>true</ns:isHidden>
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
      </ns:column>
      <ns:column columnName="Modified">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>timestamp</ns:datatype>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="ModelId">
        <ns:columnTitle>Model</ns:columnTitle>
        <ns:fk>
          <ns:fkColumnName>Name</ns:fkColumnName>
          <ns:fkTable>Models</ns:fkTable>
          <ns:fkDbSchema>simple</ns:fkDbSchema>
        </ns:fk>
        <ns:datatype>int</ns:datatype>
      </ns:column>
      <ns:column columnName="Color">
        <ns:fk>
          <ns:fkColumnName>Name</ns:fkColumnName>
          <ns:fkTable>Colors</ns:fkTable>
          <ns:fkDbSchema>simple</ns:fkDbSchema>
        </ns:fk>
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>30</ns:scale>
        <!--used in SimpleModuleTest.doTestInsertUpdateViews()-->
        <ns:shownInDetailsView>false</ns:shownInDetailsView>
      </ns:column>
      <ns:column columnName="ModelYear">
        <ns:datatype>int</ns:datatype>
      </ns:column>
      <ns:column columnName="Milage">
        <ns:datatype>int</ns:datatype>
      </ns:column>
      <ns:column columnName="LastService">
        <ns:datatype>timestamp</ns:datatype>
      </ns:column>
    </ns:columns>
  </ns:table>

  <ns:table tableName="EmissionTest" tableDbType="TABLE">
    <ns:pkColumnName>RowId</ns:pkColumnName>
    <ns:auditLogging>SUMMARY</ns:auditLogging>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:datatype>serial</ns:datatype>
        <ns:isAutoInc>true</ns:isAutoInc>
        <ns:scale>10</ns:scale>
        <ns:nullable>false</ns:nullable>
        <ns:isReadOnly>true</ns:isReadOnly>
        <ns:isKeyField>true</ns:isKeyField>
        <ns:isHidden>false</ns:isHidden>
      </ns:column>
      <ns:column columnName="Name">

      </ns:column>
      <ns:column columnName="Container">
        <ns:isUserEditable>false</ns:isUserEditable>
        <ns:datatype>entityid</ns:datatype>
        <ns:isHidden>false</ns:isHidden>
      </ns:column>
      <ns:column columnName="ParentTest">
        <ns:columnTitle>Parent Test</ns:columnTitle>
        <ns:fk>
          <ns:fkColumnName>RowId</ns:fkColumnName>
          <ns:fkTable>EmissionTest</ns:fkTable>
          <ns:fkDbSchema>vehicle</ns:fkDbSchema>
        </ns:fk>
      </ns:column>
      <ns:column columnName="VehicleId">
        <ns:fk>
          <ns:fkColumnName>RowId</ns:fkColumnName>
          <ns:fkTable>Vehicles</ns:fkTable>
          <ns:fkDbSchema>vehicle</ns:fkDbSchema>
        </ns:fk>
      </ns:column>
      <ns:column columnName="Result">
      </ns:column>
    </ns:columns>
  </ns:table>
</ns:tables>
